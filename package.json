{
  "name": "stargazer",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "emulators": "firebase emulators:start --import ./emulator-data",
    "start": "echo 'Waiting for emulators to start (`npm run emulators`)...' && wait-on http://localhost:3001 && echo 'Running & seeding...' && concurrently \"astro dev --port 3000\" \"npm run seed\"",
    "build": "astro check && astro build",
    "build-unsafe": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.5",
    "@astrojs/react": "^3.0.10",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/inter": "^5.0.16",
    "@headlessui/react": "^1.7.18",
    "@hookform/resolvers": "^3.3.4",
    "@nanostores/react": "^0.7.2",
    "@preact/signals": "^1.2.2",
    "astro": "^4.4.4",
    "date-fns": "^3.3.1",
    "firebase": "^10.8.0",
    "lodash": "^4.17.21",
    "nanostores": "^0.10.0",
    "octokit": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "ulidx": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@preact/signals-react-transform": "^0.3.0",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tailwind-merge": "^2.2.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  }
}
